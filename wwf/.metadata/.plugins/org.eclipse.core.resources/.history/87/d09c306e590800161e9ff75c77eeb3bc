package packs.model;

public class Product {
	int id,price;
	String name,desc,manufacturer,image;
	public Product()
	{ 
	}
	
	public Product(int id, int price, String name, String desc, String manufacturer, String image) {
		super();
		this.id = id;
		this.price = price;
		this.name = name;
		this.desc = desc;
		this.manufacturer = manufacturer;
		this.image = image;
	}

	public int getId() {
		return id;
	}
	public int getPrice() {
		return price;
	}
	public String getName() {
		return name;
	}
	public String getDesc() {
		return desc;
	}
	public String getManufacturer() {
		return manufacturer;
	}
	public String getImage() {
		return image;
	}
	

}
